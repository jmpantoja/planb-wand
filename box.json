{
  "alias": "wand.phar",
  "chmod": "0755",
  "directories": [
    "src",
    "legacy",
    "config"
  ],
  "files": [
    "LICENSE",
    "vendor/slevomat/coding-standard/SlevomatCodingStandard/ruleset.xml",
    "vendor/pdepend/pdepend/src/main/resources/services.xml"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": ["Tests", "tests"],
      "in": "vendor"
    },
    {
      "name": "*.xsd",
      "in": "vendor"
    },
    {
      "name": "ruleset.xml",
      "in": "vendor/squizlabs/php_codesniffer/src/Standards"
    },
    {
      "name": "*.xml",
      "in": "vendor/phpmd/phpmd/src/main/resources/rulesets"
    }
  ],
  "main": "bin/wand.php",
  "output": "build/wand.phar",
  "stub": true
}
