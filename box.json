{
  "alias": "wand.phar",
  "chmod": "0755",
  "directories": [
    "src",
    "legacy",
    "config"
  ],
  "directories-bin": [
     "vendor/bin"
  ],
  "files": [
    "LICENSE"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": ["Tests"],
      "in": "vendor"
    },
    {
      "name": "*.xsd",
      "exclude": ["Tests"],
      "in": "vendor"
    },
    {
      "name": "*.xml",
      "exclude": ["Tests"],
      "in": "vendor"
    }
  ],
  "main": "bin/wand.php",
  "output": "bin/wand.phar",
  "stub": true
}
